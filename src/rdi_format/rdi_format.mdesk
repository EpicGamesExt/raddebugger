  "//~ Format Constants";
  "";
  "// \"raddbg\\0\\0\"";
  "#define RDI_MAGIC_CONSTANT   0x0000676264646172";
  "#define RDI_ENCODING_VERSION 12";
  "";
  "////////////////////////////////////////////////////////////////";
  "//~ Format Types & Functions";
  "";
}

@h_footer
